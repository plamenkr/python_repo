iterations = int(input())
positives_list = list()
negatives_list = list()
pos_count = 0
neg_sum = 0
for i in range(iterations):
    number = int(input())
    if number >= 0:
        positives_list.append(number)
        pos_count +=1

    else:
        negatives_list.append(number)
        neg_sum += number



print(positives_list)
print(negatives_list)
print(f'Count of positives: {pos_count}')
print(f'Sum of negatives: {neg_sum}')
